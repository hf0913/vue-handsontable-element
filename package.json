{
    "name": "vue-handsontable-element",
    "version": "6.2.1",
    "description": "handsontable for vue",
    "author": "Maple <fenghu0913@gmail.com>",
    "license": "ISC",
    "main": "dist/index.js",
    "keywords": [
        "vue",
        "handsontable",
        "element",
        "excel"
    ],
    "scripts": {
        "clean": "rimraf dist",
        "build": "npm run clean && cross-env NODE_ENV=production webpack --progress --hide-modules"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "vue"
        ],
        "transform": {
            "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
            ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
        },
        "moduleNameMapper": {
            "^@/(.*)$": "<rootDir>/src/$1"
        },
        "snapshotSerializers": [
            "<rootDir>/node_modules/jest-serializer-vue"
        ],
        "mapCoverage": true
    },
    "devDependencies": {
        "@handsontable/vue": "^5.1.0",
        "@vue/test-utils": "^1.0.0-beta.16",
        "awe-dnd": "^0.3.4",
        "babel-core": "^6.26.3",
        "babel-jest": "^22.4.4",
        "babel-loader": "^9.1.0",
        "babel-plugin-syntax-jsx": "^6.18.0",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-plugin-transform-vue-jsx": "^3.7.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-latest": "^6.0.0",
        "babel-preset-stage-2": "^6.24.1",
        "cross-env": "^3.0.0",
        "css-loader": "^6.7.1",
        "custom-handsontable": "^1.2.8",
        "element-ui": "^2.13.2",
        "file-loader": "^6.2.0",
        "jest": "^22.4.4",
        "jest-serializer-vue": "^0.3.0",
        "rimraf": "^3.0.2",
        "style-loader": "^1.2.1",
        "url-loader": "^0.5.8",
        "vue": "^2.6.12",
        "vue-color": "^2.4.6",
        "vue-jest": "^2.6.0",
        "vue-loader": "^11.1.4",
        "vue-template-compiler": "^2.6.12",
        "vuelidate": "^0.5.0",
        "webpack": "^5.74.0",
        "webpack-dev-server": "^2.11.2"
    },
    "dependencies": {}
}
